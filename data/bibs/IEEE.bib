  month={Feb},}@INPROCEEDINGS{11171936,
  author={Akter, Aklima and Siam, Tanvir and Alam, Md. Sabbir and Karim, Fazlul and Saha, Kiron and Rony, Jahidul Hasan and Momo, Nafiza Khanom and Mansur, Ahmed Al},
  booktitle={2025 International Conference on Quantum Photonics, Artificial Intelligence, and Networking (QPAIN)}, 
  title={Site Suitability Assessment for Solar PV Installation in Bangladesh: Analyzing Long-Term PM10 Dust and Solar Irradiance Data}, 
  year={2025},
  volume={},
  number={},
  pages={1-6},
  abstract={The air quality in Bangladesh is characterized by significantly higher levels of dust compared to many developed nations, with various anthropogenic sources contributing to this pollution. Despite its significant impact on public health and the environment, there are limited measures in place to address dust emissions. This study examines the long-term dust levels in urban regions of Bangladesh, highlighting the excessive presence of PM10 particles. A novel aspect of this work lies in the integration of both dust and solar radiation data to assess the feasibility of solar power plant locations in the country. The study emphasizes the importance of selecting sites with minimal dust sources nearby, such as those distanced from major industrial zones, brick kilns, and large-scale construction sites. Additionally, it proposes that regional air quality be considered as a critical parameter in environmental impact assessments for new developments. The Government of Bangladesh's (GOB) 2025 Renewable Energy (RE) policy places substantial focus on solar energy, making this analysis timely and relevant. The research evaluates six locations—Dhaka, Barisal, Rangpur, Chittagong, Sylhet, and Rajshahi—based on the annual average PM10 dust concentration and solar radiation levels from 2015 to 2024. The results indicate that solar radiation ranged from 147.667 to $441 \text{kWh} / \mathrm{m}^{2}$, while PM10 concentrations varied between 106.1 and $153.8 ~ \mu ~\mathrm{g} / \mathrm{m}^{3}$. This work identifies optimal sites for solar photovoltaic (PV) installations in Bangladesh, addressing the dual challenges of dust pollution and solar irradiance, and offers novel insights for sustainable solar energy deployment.},
  keywords={Solar irradiance;Renewable energy sources;Pollution;Prevention and mitigation;Air quality;Real-time systems;Solar radiation;Solar panels;Sustainable development;Public healthcare;Solar panels;air quality;environment;PM10;dust;solar radiation},
